{
  "name": "sinh-phuc-tho",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cp-env-dev-maitanghcm": "copy .env.dev.maitanghcm .env",
    "cp-env-dev-sinhphuctho": "copy .env.dev.sinhphuctho .env",
    "cp-env-prod-maitanghcm": "copy .env.prod.maitanghcm .env",
    "cp-env-prod-sinhphuctho": "copy .env.prod.sinhphuctho .env",
    "dev:sinhphuctho": "npm run cp-env-dev-sinhphuctho && next dev",
    "dev:maitanghcm": "npm run cp-env-dev-maitanghcm && next dev",
    "build:sinhphuctho": "npm run cp-env-prod-sinhphuctho && npm run generate-album && next build && npm run post-build",
    "build:maitanghcm": "npm run cp-env-prod-maitanghcm && npm run generate-album && next build && npm run post-build",
    "start": "next start -p 3006",
    "start:static": "npx serve@latest out -p 3007",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint",
    "deploy:sinhphuctho": "aws s3 sync out s3://sinh-phuc-tho-host",
    "deploy:maitanghcm": "aws s3 sync out s3://maitanghcm-host",
    "post-build": "next-sitemap",
    "generate-album": "ts-node --project tsconfig.scripts.json scripts/generateAlbum.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/react-slick": "^1.1.2",
    "@emailjs/browser": "^4.4.1",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^5.16.6",
    "@next/bundle-analyzer": "^14.2.5",
    "antd": "^5.20.1",
    "iconify": "^1.4.0",
    "iconsax-react": "^0.0.8",
    "next": "14.2.5",
    "next-sitemap": "^4.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-grid-gallery": "^1.0.1",
    "react-loading-skeleton": "^3.5.0",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@iconify/react": "^5.0.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "compression-webpack-plugin": "^11.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "next-export-optimize-images": "^4.5.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
